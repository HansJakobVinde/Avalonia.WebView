<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:viewModels="clr-namespace:SampleWebView.ViewModels"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="SampleWebView.Views.MainView" >

	<Grid RowDefinitions="50,*">

		<StackPanel Orientation="Horizontal" Spacing="10">
			<Button x:Name="PART_Button" Content="Baidu" Command="{Binding BaiduCommand}"/>
			<Button x:Name="PART_Button1" Content="IconFont" Command="{Binding IconFontCommand}"/>
		</StackPanel>
	
		<WebView x:Name="PART_WebView" Grid.Row="1" Url="{Binding Url}"></WebView>

	</Grid>
</UserControl>
